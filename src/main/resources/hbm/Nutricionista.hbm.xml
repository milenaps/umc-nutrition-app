<?xml version="1.0" encoding="UTF-8"?> 
<!DOCTYPE hibernate-mapping PUBLIC "-//Hibernate/Hibernate Mapping DTD 3.0//EN" "http://hibernate.sourceforge.net/hibernate-mapping-3.0.dtd">
<hibernate-mapping>
	<class name="main.java.com.nutri.model.Nutricionista" table="nutricionista">
		<id name="id" column="id" unsaved-value="0">
			<generator class="increment" />
		</id>
		<property name="crn" column="crn" not-null="true" unique="true" />
		<property name="aprovado" column="aprovado" type="java.lang.Boolean" not-null="true" />
	</class>
</hibernate-mapping>