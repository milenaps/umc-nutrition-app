<?xml version="1.0" encoding="UTF-8"?> 
<!DOCTYPE hibernate-mapping PUBLIC "-//Hibernate/Hibernate Mapping DTD 3.0//EN" "http://hibernate.sourceforge.net/hibernate-mapping-3.0.dtd">
<hibernate-mapping>
	<class name="com.nutri.java.model.HistoricoAlimentar" table="historicoalimentar">
		<id name="id" column="id" unsaved-value="0">
			<generator class="increment" />
		</id>
		<property name="data" column="data" type="java.util.Date" not-null="true" />
		<property name="hora" column="hora" not-null="true" />
		<property name="quantidade" column="quantidade" not-null="true"
			type="java.lang.Double" />
	</class>
</hibernate-mapping>